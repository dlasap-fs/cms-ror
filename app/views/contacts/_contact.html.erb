<div id="<%= dom_id contact %>">
  <p>
    <strong>First name:</strong>
    <%= contact.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= contact.last_name %>
  </p>

  <p>
    <strong>Physical address:</strong>
    <%= contact.physical_address %>
  </p>

  <p>
    <strong>Billing address:</strong>
    <%= contact.billing_address %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= contact.email %>
  </p>

</div>
